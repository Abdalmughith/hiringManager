<!DOCTYPE html>
<html>
  <head>
    <title>cv</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
  	<script  type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>
	<script  type="text/javascript" src="http://getbootstrap.com/dist/js/bootstrap.min.js"></script>
	<script src="//cdnjs.cloudflare.com/ajax/libs/jquery/2.1.3/jquery.min.js"></script>
	<script src="//maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js"></script>
	<script  type="text/javascript" src="./js.js"></script>

	<link rel="stylesheet" type="text/css" href="./css.css">
  </head>
  <body>
    <div class="row">
    <!-- uncomment code for absolute positioning tweek see top comment in css -->
    <!-- <div class="absolute-wrapper"> </div> -->
    <!-- Menu -->
    <div class="side-menu">
    
    <nav class="navbar navbar-default" role="navigation" style="height: 100%;    overflow-x: auto;">
    <!-- Brand and toggle get grouped for better mobile display -->
    <div class="navbar-header">
        <div class="brand-wrapper">
            <!-- Hamburger -->
            <button type="button" class="navbar-toggle">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>

            <!-- Brand -->
            <div class="brand-name-wrapper">
                <a class="navbar-brand" href="#">
                    Result
                </a>
            </div>

        </div>

    </div>

    <!-- Main Menu -->
    <div class="side-menu-container">
        <ul class="nav navbar-nav">

        <% positions.forEach(function(pos){ %>
            <li><a href="/result?name=<%=pos.title%>"><span class="glyphicon glyphicon-send"></span> <%=pos.title%></a></li>
        <% }); %>
           

        </ul>
    </div><!-- /.navbar-collapse -->
</nav>
    
    </div>

    <!-- Main Content -->
    <div class="container-fluid">
        <div class="side-body">
           <%if(typeof result != 'undefined'){%>
                <table class="table table-bordered">
                    <tr>
                        <th>name</th>
                        <th>age</th>
                        <th>Rank</th> 
                    </tr>

                    <% _.forEachRight(result,function(person){%>
                        <tr>
                            <td><%= person.person.title %></td>
                            <td><%= person.person.age %></td>
                            <td><%= person.rank %></td>
                        </tr>
                    <% }); %>
                </table>
           <%}%>
         
        </div>
    </div>
</div>
	<script  type="text/javascript" src="./js.js"></script>
  </body>
 </html>
